{
  "name": "@ace/root",
  "private": true,
  "version": "1.0.0",
  "devDependencies": {
    "lerna": "^5.1.4",
    "nx": "14.3.6"
  },
  "scripts": {
    "build": "nx run-many --parallel --target=build --projects=@ace/admin-table,@ace/website",
    "clean": "nx run-many --parallel --target=clean --projects=@ace/admin-table,@ace/website",
    "diff": "lerna diff",
    "format": "nx format --all",
    "new-version": "lerna version --conventional-commits --yes",
    "nx:graph": "nx dep-graph",
    "serve": "nx run-many --parallel --target=serve --projects=@ace/admin-table,@ace/website",
    "start": "nx run-many --parallel --target=start --projects=@ace/admin-table,@ace/website",
    "start:admintable": "nx start @ace/admin-table",
    "start:website": "nx start @ace/website",
    "test": "lerna run test",
    "build:vercel": "ncc build scripts/vercel.js -o dist"
  },
  "dependencies": {
    "@ant-design/icons": "^4.7.0",
    "@reach/router": "^1.3.4",
    "@testing-library/react": "^13.3.0",
    "@vercel/ncc": "^0.34.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-lottie": "^1.2.3"
  },
  "workspaces": [
    "packages/*"
  ]
}
